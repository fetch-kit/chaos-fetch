{
  "name": "@fetchkit/chaos-fetch",
  "version": "0.7.0",
  "description": "Client-side chaos-testing library for JavaScript/TypeScript apps.",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prepare": "husky install",
    "test:ci": "vitest --run",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write .",
    "changeset": "changeset",
    "version": "changeset version",
    "prepublishOnly": "npm run build && npm run test:ci"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chaos",
    "fetch",
    "testing",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/router": "^14.0.0",
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/koa__router": "^12.0.4",
    "@types/koa-compose": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
